{
  "name":                                     "austral/tools-bundle",
  "type":                                     "symfony-bundle",
  "description":                              "Austral Tools function and Abstract Class squelette",
  "keywords":                                 ["Austral", "Tools"],
  "license":                                  "MIT",
  "homepage":                                 "https://austral.dev",
  "version":                                  "3.1",
  "authors": [
    {
      "name":                                 "Matthieu Beurel",
      "email":                                "matthieu@austral.dev",
      "role":                                 "Developer"
    }
  ],
  "require": {
    "php":                                    "^7.4 || ^8.0",

    "symfony/console":                        "^5.4",
    "symfony/dotenv":                         "^5.4",
    "symfony/framework-bundle":               "^5.4",
    "symfony/process":                        "^5.4",
    "symfony/serializer":                     "^5.4",
    "symfony/string":                         "^5.4",
    "symfony/yaml":                           "^5.4",

    "twig/twig":                              "^3.0",
    "twig/extra-bundle":                      "^3.0",
    "twig/string-extra":                      "^3.2",

    "gedmo/doctrine-extensions":              "^3.0",
    "ramsey/uuid":                            "^3.0",
    "adbario/php-dot-notation":               "^2.2"
  },
  "require-dev": {
    "symfony/css-selector":                   "^5.4",
    "symfony/phpunit-bridge":                 "^5.4",
    "symfony/browser-kit":                    "^5.4",
    "symfony/debug-bundle":                   "^5.4",
    "symfony/stopwatch":                      "^5.4",
    "symfony/var-dumper":                     "^5.4",
    "symfony/web-profiler-bundle":            "^5.4"
  },
  "config": {
    "sort-packages":                          true
  },
  "autoload": {
    "psr-4": { "Austral\\ToolsBundle\\": "" }
  },
  "minimum-stability":                        "dev"
}
